{
  "git": {
    "deploymentEnabled": {
      "main": false
    }
  },
  "rewrites": [{ "source": "/**", "destination": "/index.html" }],
  "builds": [
    {
      "src": "apps/frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "apps/frontend/dist" }
    }
  ]
}
